<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <report
        id="client_report"
        model="res.partner"
        report_type="qweb-pdf"
        string="Tee Report"
        name="Qweb_pdf.export_in_pdf"
        file="Qweb_pdf.export_in_pdf"
    />

    <template id="export_in_pdf">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="o" >
                <t t-call="web.internal_layout">
                    <div class="page">
                        <h1>
                            Client information
                        </h1>
                        <p><strong>Name:</strong> <t t-esc="o.name"/></p>
                        <p><strong>Email:</strong> <t t-esc="o.email"/></p>
                        <p><strong>Phone:</strong> <t t-esc="o.phone"/></p>

                    </div>
                </t>
            </t>

        </t>
    </template>


</odoo>
